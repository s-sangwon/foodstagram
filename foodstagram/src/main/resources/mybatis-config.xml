<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="cacheEnabled" value="false"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.circle.foodstagram.member.model.vo.Member" alias="Member" />
		<typeAlias type="com.circle.foodstagram.notice.model.vo.Notice" alias="Notice" />
		<typeAlias type="com.circle.foodstagram.common.SearchDate" alias="SearchDate" />
		<typeAlias type="com.circle.foodstagram.common.Paging" alias="Paging" />
		<typeAlias type="com.circle.foodstagram.common.SearchPaging" alias="SearchPaging" />
		<typeAlias type="com.circle.foodstagram.common.FileDownloadView" alias="FileDownloadView" />
		<typeAlias type="com.circle.foodstagram.qna.model.vo.Question" alias="Question" />
		<typeAlias type="com.circle.foodstagram.qna.model.vo.Answer" alias="Answer" />
		<typeAlias type="com.circle.foodstagram.common.attach.model.vo.Attach" alias="Attach" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/member-mapper.xml" />
		<mapper resource="mappers/notice-mapper.xml" />
		<mapper resource="mappers/question-mapper.xml" />
		<mapper resource="mappers/answer-mapper.xml" />
		<mapper resource="mappers/attach-mapper.xml" />
	</mappers>
</configuration>
